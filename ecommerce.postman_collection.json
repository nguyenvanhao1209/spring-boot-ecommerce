					"name": "login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var response = pm.response.json();\r",
									"\r",
									"// Extract the accessToken\r",
									"var accessToken = response.accessToken;\r",
									"var refreshToken = response.refreshToken;\r",
									"\r",
									"// Set the accessToken as a collection variable\r",
									"pm.collectionVariables.set(\"token\", accessToken);\r",
									"pm.collectionVariables.set(\"refresh\", refreshToken)"
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
